      <Field dataOrder="31" name="Month" dataType="System.Byte" actualDataType="UInt8" numBytes="1" />
      <Field dataOrder="32" name="Day" dataType="System.Byte" actualDataType="UInt8" numBytes="1" />
      <Field dataOrder="33" name="Hour" dataType="System.Byte" actualDataType="UInt8" numBytes="1" />
      <Field dataOrder="34" name="Minute" dataType="System.Byte" actualDataType="UInt8" numBytes="1" />
      <Field dataOrder="35" name="Second" dataType="System.Byte" actualDataType="UInt8" numBytes="1" />
      <Field dataOrder="36" name="MilliSecond" dataType="System.UInt16" actualDataType="UInt16" numBytes="2" />

