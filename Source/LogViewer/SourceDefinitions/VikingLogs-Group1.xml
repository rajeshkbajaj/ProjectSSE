    <!-- Rendering describes how to generate an output file -->
    <RenderDescriptions>
      <RenderDescription name="Simple" >
        <RenderTableAttribs>border="1" id="XX-Tbl" summary="Div Schedule"</RenderTableAttribs>
        <RenderTableHeader>
            <thead>
              <tr>
                <th>#</th>
                <th>Date</th>
                <th>Time</th>
                <th>Patient Id</th>
                <th>Obj Id</th>
                <th>User Interaction Id</th>
              </tr>
            </thead>
          </RenderTableHeader>
        <RenderTableRow>
          <tr>
            <td align="right">%0%</td>
            <td>%2%-%3%-%1%</td>
            <td align="right">%4%:%5%:%6%.%7%</td>
            <td>%8%</td>
            <td>%10%</td>
            <td>%11%</td>
          </tr>
        </RenderTableRow>
        <RenderField name="Year" order="1" header="Year" format="'{0:00}'" />
        <RenderField name="Month" order="2" header="Mon" format="'{0:00}'" />
