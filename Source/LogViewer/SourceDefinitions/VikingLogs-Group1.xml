      <Field dataOrder="8" name="BaseChecksum" dataType="System.UInt32" actualDataType="UInt32" numBytes="4" />
      <Field dataOrder="9" name="DiagnosticCode" dataType="System.Byte" actualDataType="DiagnosticCode" numBytes="12" isUnionSet="DiagnosticCodes" />
      <Field dataOrder="10" name="RecordChecksum" dataType="System.UInt32" actualDataType="UInt32" numBytes="4" />
    </fields>

    
    <!-- Unions are individual definitions of sub-structures which will be C-Unioned (overlayed on the same memory) -->
    <!-- Bit Offsets are relative to the Byte in which they begin -->

    <unions>
      <union name="SystemEventBits" numBytes="12" >
        <field name="diagId" dataOrder="0" dataType="System.Byte" actualDataType="Uint8" bitOffset="0" bitLength="4" />
        <field name="cpuFlagId" dataOrder="1" dataType="System.Byte" actualDataType="Uint8" bitOffset="4" bitLength="2" />
        <field name="systemEventId" dataOrder="2" dataType="System.Byte" actualDataType="Uint8" bitOffset="6" bitLength="10" />
        <field name="UNUSED_WORD" dataOrder="3" dataType="System.UInt16" actualDataType="UInt16" numBytes="2" />
        <field name="auxEventCode" dataOrder="4" dataType="System.UInt32" actualDataType="UInt32" numBytes="4" />
        <field name="UNUSED_LWORD4" dataOrder="5" dataType="System.UInt32" actualDataType="UInt32" numBytes="4" />
        <RenderField name="diagId" order="1" header="Diag Id" />
        <RenderField name="cpuFlagId" order="2" header="CPU" />
        <RenderField name="systemEventId" order="3" header="Sys Evt Id" />
        <RenderField name="auxEventCode" order="4" header="Aux Evt Code" />
        <RenderExtra name="DiagCode" order="5" header="Diag Code" PreFormat="'(1==%1%?E:S){0:00000}'" Elements="1" />
      </union>
      <union name="SoftwareTestBits" numBytes="12" >
        <field name="diagId" dataOrder="0" dataType="System.Byte" actualDataType="Uint8" bitOffset="0" bitLength="4" />
        <field name="cpuFlagId" dataOrder="1" dataType="System.Byte" actualDataType="Uint8" bitOffset="4" bitLength="2" />
        <field name="assertionId" dataOrder="2" dataType="System.Byte" actualDataType="Uint8" bitOffset="6" bitLength="3" />
        <field name="UNUSED_BITS" dataOrder="3" dataType="System.Byte" actualDataType="Uint8" bitOffset="1" bitLength="7" />
        <field name="activeTaskId" dataOrder="4" dataType="System.Byte" actualDataType="Uint8" numBytes="1" />
        <field name="subsystemId" dataOrder="5" dataType="System.Byte" actualDataType="Uint8" numBytes="1" />
        <field name="moduleId" dataOrder="6" dataType="System.UInt16" actualDataType="UInt16" numBytes="2" />
        <field name="lineNumber" dataOrder="7" dataType="System.UInt16" actualDataType="UInt16" numBytes="2" />
        <field name="auxErrorCode" dataOrder="8" dataType="System.UInt32" actualDataType="UInt32" numBytes="4" />
        <RenderField name="diagId" order="1" header="Diag Id" />
        <RenderField name="cpuFlagId" order="2" header="CPU" />
        <RenderField name="assertionId" order="3" header="Assert Id" />
