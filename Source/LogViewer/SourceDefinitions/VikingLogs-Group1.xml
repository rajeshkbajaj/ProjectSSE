            <td align="right">%0%</td>
            <td>%2%-%3%-%1%</td>
            <td align="right">%4%:%5%:%6%.%7%</td>
            <td>%8%</td>
            <td>%9%</td>
            <td>%10%</td>
            <td>%11%</td>
            <td>%12%</td>
          </tr>
        </RenderTableRow>
        <RenderField name="Year" order="0" header="Year" format="20{0:00}" />
        <RenderField name="Month" order="1" header="Mon" format="{0:00}" />
        <RenderField name="Day" order="2" header="Day" format="{0:00}" />
        <RenderField name="Hour" order="3" header="Hr" format="{0:00}" />
        <RenderField name="Minute" order="4" header="Min" format="{0:00}" />
        <RenderField name="Second" order="5" header="Sec" format="{0:00}" />
        <RenderField name="MilliSecond" order="6" header="mSec" format="{0:000}" />
        <RenderField name="PatientId" order="7" header="Patient Id" />
        <RenderField name="BaseChecksum" order="8" header="Base C-Sum" />
        <RenderField name="ObjectId" order="9" header="Obj Id" />
        <RenderField name="UserInteractionId" order="10" header="UI Id" />
        <RenderField name="RecordChecksum" order="11" header="Rec C-sum" />
      </RenderDescription>
    </RenderDescriptions>

  </LogDefinition>


