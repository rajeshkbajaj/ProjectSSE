    <!-- Fields map to the actual bit/byte structure of the raw message - embedded types folded in -->
    <fields>
      <Field dataOrder="0" name="Year" dataType="System.Byte" actualDataType="UInt8" numBytes="1" />
      <Field dataOrder="1" name="Month" dataType="System.Byte" actualDataType="UInt8" numBytes="1" />
      <Field dataOrder="2" name="Day" dataType="System.Byte" actualDataType="UInt8" numBytes="1" />
