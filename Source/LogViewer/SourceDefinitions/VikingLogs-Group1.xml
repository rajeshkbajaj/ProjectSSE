          <thead>
            <tr>
              <th>#</th>
              <th>Date</th>
              <th>Time</th>
              <th>Patient Id</th>
              <th>Base C-Sum</th>
              <th>Obj Id</th>
              <th>User Interaction Id</th>
              <th>Rec C-sum</th>
